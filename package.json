{
    "name": "lib-demo",
    "version": "0.0.1",
    "author": "Scott Robertson (https://github.com/sc0tt5)",
    "license": "GNU GPL v3",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open --watch",
        "build": "ng build --aot",
        "build:prod": "rimraf dist && npm run lib && ng build --prod && ng serve --prod --open",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "doc": "compodoc -p tsconfig.app.json -d tmp/documentation/demo",
        "lib:doc": "compodoc -p projects/my-lib/tsconfig.lib.json -d tmp/documentation/lib",
        "lib:version": "npm version patch --prefix projects/my-lib",
        "lib:start": "rimraf dist/my-lib && ng build my-lib --watch",
        "lib:build": "rimraf dist && ng build my-lib",
        "lib:pack": "cd dist/my-lib && npm pack",
        "lib:install": "npm install dist/my-lib",
        "lib:styles": "cpx \"./projects/my-lib/src/lib/assets/styles/**/*.scss\" \"./dist/my-lib/assets/styles/\"",
        "lib": "npm run lib:build && npm run lib:styles && npm run lib:pack && npm run lib:install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.3",
        "@angular/common": "~8.2.3",
        "@angular/compiler": "~8.2.3",
        "@angular/core": "~8.2.3",
        "@angular/forms": "~8.2.3",
        "@angular/platform-browser": "~8.2.3",
        "@angular/platform-browser-dynamic": "~8.2.3",
        "@angular/router": "~8.2.3",
        "@my-lib/components": "file:dist/my-lib",
        "rxjs": "~6.5.2",
        "tslib": "^1.10.0",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.0",
        "@angular-devkit/build-ng-packagr": "~0.803.0",
        "@angular/cli": "^8.1.1",
        "@angular/compiler-cli": "~8.2.3",
        "@angular/language-service": "~8.2.3",
        "@compodoc/compodoc": "^1.1.10",
        "@fortawesome/angular-fontawesome": "0.5.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.22",
        "@fortawesome/free-solid-svg-icons": "^5.10.2",
        "@types/jasmine": "~3.4.0",
        "@types/jasminewd2": "~2.0.6",
        "@types/node": "~12.7.2",
        "bootstrap": "4.3.1",
        "codelyzer": "^5.1.0",
        "cpx": "^1.5.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.2.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ng-packagr": "^10.1.1",
        "ngx-bootstrap": "^5.1.1",
        "protractor": "~5.4.2",
        "rimraf": "^3.0.0",
        "ts-node": "~8.3.0",
        "tsickle": "^0.35.0",
        "tslint": "~5.19.0",
        "typescript": "~3.4.3"
    }
}
